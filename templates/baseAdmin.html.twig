<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge"/>
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no"/>
    <title>{% block title %}Administration Burger King{% endblock %}</title>

    {# Run `composer require symfony/webpack-encore-bundle`
       and uncomment the following Encore helpers to start using Symfony UX #}
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/fullcalendar@5.7.2/main.min.css" integrity="sha256-u40zn9KeZYpMjgYaxWJccb4HnP0i8XI17xkXrEklevE=" crossorigin="anonymous">
    {% block stylesheets %}
        {#{{ encore_entry_link_tags('app') }}#}

        <link href="{{ asset('admin/assets/css/pace.min.css') }}" rel="stylesheet"/>
        <!--favicon-->
        <link rel="icon" href="{{ asset('admin/assets/images/favicon.ico') }}" type="image/x-icon">
        <!-- Vector CSS -->
        <link href="{{ asset('admin/assets/plugins/vectormap/jquery-jvectormap-2.0.2.css') }}" rel="stylesheet"/>
        <!-- simplebar CSS-->
        <link href="{{ asset('admin/assets/plugins/simplebar/css/simplebar.css') }}" rel="stylesheet"/>
        <!-- Bootstrap core CSS-->
        <link href="{{ asset('admin/assets/css/bootstrap.min.css') }}" rel="stylesheet"/>
        <!-- animate CSS-->
        <link href="{{ asset('admin/assets/css/animate.css') }}" rel="stylesheet" type="text/css"/>
        <!-- Icons CSS-->
        <link href="{{ asset('admin/assets/css/icons.css') }}" rel="stylesheet" type="text/css"/>
        <!-- Sidebar CSS-->
        <link href="{{ asset('admin/assets/css/sidebar-menu.css') }}" rel="stylesheet"/>
        <!-- Custom Style-->
        <link href="{{ asset('admin/assets/css/app-style.css') }}" rel="stylesheet"/>
        <!-- FontAwesome-->
        <link href="{{ asset('admin/assets/css/all.css') }}" rel="stylesheet"/>
        <link href="{{ asset('admin/assets/css/calendar.css') }}" rel="stylesheet"/>

    {% endblock %}

    <script src="https://cdn.jsdelivr.net/npm/fullcalendar@5.7.2/main.min.js" integrity="sha256-yirUYbNvdsLHfZcQDyDMB51pfQ0Mn8siGDZOvtBgCFw=" crossorigin="anonymous"></script>
    {% block javascripts %}

        <script src="{{ asset('admin/assets/js/jquery.min.js') }}"></script>
        <script src="{{ asset('admin/assets/js/popper.min.js') }}"></script>
        <script src="{{ asset('admin/assets/js/bootstrap.min.js') }}"></script>
        <script src="{{ asset('admin/assets/js/pace.min.js') }}"></script>

        <!-- simplebar js -->
        <script src="{{ asset('admin/assets/plugins/simplebar/js/simplebar.js') }}"></script>
        <!-- sidebar-menu js -->
        <script src="{{ asset('admin/assets/js/sidebar-menu.js') }}"></script>
        <!-- loader scripts -->
        <script src="{{ asset('admin/assets/js/jquery.loading-indicator.js') }}"></script>
        <!-- Custom scripts -->
        <script src="{{ asset('admin/assets/js/app-script.js') }}"></script>
        <!-- Chart js -->

        <script src="{{ asset('admin/assets/plugins/Chart.js/Chart.min.js') }}"></script>

        <!-- Index js -->
        <script src="{{ asset('admin/assets/js/index.js') }}"></script>
        <script src="{{ asset('admin/assets/js/calendar.js') }}"></script>

    {% endblock %}
</head>
<body class="bg-theme bg-theme2">

<!--Start sidebar-wrapper-->
<div id="sidebar-wrapper" data-simplebar="" data-simplebar-auto-hide="true">
    <div class="brand-logo">
        <a href="baseAdmin.html.twig">
            <i class="fas fa-hamburger fa-2x"></i>
            <h5 class="logo-text">Burger King Admin</h5>
        </a>
    </div>
    <ul class="sidebar-menu do-nicescrol">
        <li class="sidebar-header">NAVIGATION</li>
        <li>
            <a href="{{ path('menu_index') }}">
                <i class="zmdi zmdi-view-dashboard"></i> <span>Menu</span>
            </a>
        </li>

        <li>
            <a href="{{ path('categories_index') }}">
                <i class="fas fa-bars"></i> <span>Catégories</span>
            </a>
        </li>

        <li>
            <a href="{{ path('image_index') }}">
                <i class="fas fa-images"></i> <span>Image</span>
            </a>
        </li>

        <li>
            <a href="{{ path('produit_index') }}">
                <i class="fas fa-utensils"></i> <span>Produit</span>
            </a>
        </li>

        <li>
            <a href="{{ path('features_index') }}">
                <i class="far fa-flag"></i> <span>Features</span>
            </a>
        </li>

        <li>
            <a href="{{ path('chef_index') }}">
                <i class="far fa-address-card"></i> <span>Chef</span>
            </a>
        </li>

        <li>
            <a href="{{ path('about_index') }}">
                <i class="fas fa-book-open"></i> <span>About</span>
            </a>
        </li>

        <li>
            <a href="{{ path('contact_index') }}">
                <i class="far fa-envelope"></i> <span>Boite de Réception</span>
            </a>
        </li>
        <li>
            <a href="{{ path('calendrier') }}">
                <i class="far fa-calendar-check"></i> <span>Calendrier</span>
            </a>
        </li>

    </ul>

</div>

{% block body %}

{% endblock %}


</body>
</html>